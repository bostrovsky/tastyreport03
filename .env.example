# Django Settings
SECRET_KEY=your-secret-key-here
DEBUG=True

# Database Configuration
POSTGRES_DB=tastytrade_tracker
POSTGRES_USER=your_username
POSTGRES_PASSWORD=your_password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# TastyTrade OAuth 2.0 Configuration
# Register your OAuth application with TastyTrade to get these values
TASTYTRADE_OAUTH_CLIENT_ID=your_client_id_from_tastytrade
TASTYTRADE_OAUTH_CLIENT_SECRET=your_client_secret_from_tastytrade
TASTYTRADE_OAUTH_REDIRECT_URI=http://localhost:8000/tastytrade/oauth/callback/

# Email Configuration (Optional)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USE_TLS=True
EMAIL_HOST_USER=your_email@gmail.com
EMAIL_HOST_PASSWORD=your_app_password